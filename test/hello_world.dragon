[ entry point ]
dragon.main()() = {
    [ initialization message ]
    dragon.set("The dragon slumbers...%0a;")(message)
    dragon.print.buffer_as_string(message)()

    [ test compiler ]
    [ compile ]
    dragon.set("{{{}}}%22;[{}DSA{Dskjhnfa}%22;")(text)
    tools.json.compile(text)(error)

    [ display message ]
    tools.json.error.print_if_occured(error)()
    dragon.print.new_line()()
}
